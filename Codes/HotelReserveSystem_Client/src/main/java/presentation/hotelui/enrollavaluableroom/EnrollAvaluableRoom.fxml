<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
	prefWidth="1000.0" styleClass="background" stylesheets="@../../DarkTheme.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="avaluableRoomListLabel" layoutX="100.0" layoutY="100.0"
			prefHeight="50.0" prefWidth="300.0" styleClass="label-title" text="可用客房列表" />

		<HBox layoutX="550.0" layoutY="600.0" spacing="50.0">
			<children>
				<Button fx:id="cancelButton" layoutX="600.0" layoutY="600.0"
					mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="取消" />
				<Button fx:id="confirmButton" layoutX="797.0" layoutY="624.0"
					mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="确认" />
			</children>
		</HBox>
		<TableView fx:id="avaluableRoomInfo" layoutX="200.0"
			layoutY="200.0" prefHeight="300.0" prefWidth="600.0" styleClass="table-view-small">
			<columns>
				<TableColumn prefWidth="200.0" text="房间类型" />
				<TableColumn prefWidth="200.0" text="房间数量" />
				<TableColumn prefWidth="200.0" text="价格" />
			</columns>
		</TableView>
		<HBox layoutX="500.0" layoutY="130.0" spacing="50.0">
			<children>
				<Button fx:id="addButton" layoutX="617.0" layoutY="162.0"
					mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" text="添加新房型" />
				<Button fx:id="editButton" layoutX="785.0" layoutY="174.0"
					mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="编辑" />
			</children>
		</HBox>

	</children>
</AnchorPane>
