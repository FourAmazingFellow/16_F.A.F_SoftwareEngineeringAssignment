<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1000.0" styleClass="background" stylesheets="@Theme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar fx:id="menu" prefHeight="50.0" prefWidth="800.0" styleClass="\\@Theme.css">
         <menus>
            <Menu fx:id="return" mnemonicParsing="false" styleClass="\@Theme.css" text="返回" />
            <Menu fx:id="homepage" mnemonicParsing="false" styleClass="\@Theme.css" text="主页" />
         </menus>
      </MenuBar>
      <HBox spacing="10.0" />
      <TabPane fx:id="tab" layoutY="50.0" prefHeight="50.0" prefWidth="1000.0" styleClass="background" stylesheets="@Theme.css" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab fx:id="manageHotelInfo" styleClass="\@Theme.css" text="维护酒店基本信息" />
            <Tab fx:id="queryOrders" text="浏览订单" />
            <Tab fx:id="queryEmptyRoom" styleClass="\@Theme.css" text="浏览空房" />
            <Tab fx:id="checkIn" styleClass="\@Theme.css" text="办理入住" />
            <Tab fx:id="checkOut" styleClass="\@Theme.css" text="办理退房" />
            <Tab fx:id="managePromotion" styleClass="\@Theme.css" text="管理促销策略" />
            <Tab fx:id="enrollAvaluableRoom" styleClass="\@Theme.css" text="录入可用客房" />
         </tabs>
      </TabPane>
      <ComboBox fx:id="username" layoutX="800.0" prefHeight="50.0" prefWidth="200.0" promptText="用户名" styleClass="\@Theme.css" visibleRowCount="4">
         <cursor>
            <Cursor fx:constant="NONE" />
         </cursor>
      </ComboBox>
   </children>
</AnchorPane>
