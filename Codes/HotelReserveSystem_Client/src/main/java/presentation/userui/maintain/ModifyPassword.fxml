<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="640.0"
	prefWidth="1000.0" styleClass="background"
	stylesheets="@../../DarkTheme.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="presentation.userui.maintain.ModifyPasswordController">
	<children>
		<Label fx:id="passwordTitleLabel" layoutX="148.0" layoutY="70.0"
			prefHeight="50.0" prefWidth="250.0" styleClass="label-title" text="修改密码" />

		<HBox layoutX="298.0" layoutY="520.0" spacing="100.0">
			<children>
				<Button fx:id="cancelButton" layoutX="274.0" layoutY="445.0"
					mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="50.0"
					prefWidth="150.0" text="取消" />
				<Button fx:id="confirmButton" layoutX="399.0" layoutY="434.0"
					mnemonicParsing="false" onAction="#confirmButtonAction" prefHeight="50.0"
					prefWidth="150.0" text="确认" />

			</children>
		</HBox>
		<GridPane layoutX="198.0" layoutY="170.0" prefHeight="300.0"
			prefWidth="600.0" styleClass="label-bright">
			<columnConstraints>
				<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
					minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints halignment="CENTER" hgrow="SOMETIMES"
					minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label styleClass="label-bright" text="原密码" GridPane.rowIndex="1" />
				<Label styleClass="label-bright" text="确认新密码"
					GridPane.rowIndex="3" />
				<Label styleClass="label-bright" text="新密码" GridPane.rowIndex="2" />
				<TextField fx:id="prePasswordField" GridPane.columnIndex="1"
					GridPane.rowIndex="1" />
				<TextField fx:id="newPasswordConfirmField"
					GridPane.columnIndex="1" GridPane.rowIndex="3" />
				<TextField fx:id="newPasswordField" GridPane.columnIndex="1"
					GridPane.rowIndex="2" />
				<Label styleClass="label-bright" text="用户名" />
				<Label fx:id="userIDLabel" styleClass="label-bright" text="userName"
					GridPane.columnIndex="1" />
			</children>
		</GridPane>
	</children>
</AnchorPane>
