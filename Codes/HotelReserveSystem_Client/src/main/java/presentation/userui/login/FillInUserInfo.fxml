<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" styleClass="background" stylesheets="@../../DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.userui.login.FillInUserInfoController">
	<children>
		<Label fx:id="userInfoLabel" layoutX="100.0" layoutY="50.0" styleClass="label-header" text="填写用户基本信息" />
		<GridPane fx:id="userInfoPane" layoutX="300.0" layoutY="200.0" prefHeight="200.0" prefWidth="400.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints />
				<RowConstraints />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="用户类型" GridPane.rowIndex="2" />
				<ChoiceBox fx:id="userTypeChoiceBox" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<Label text="联系方式" GridPane.rowIndex="3" />
				<TextField fx:id="telNumField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
			</children>
		</GridPane>
		<HBox layoutX="300.0" layoutY="500.0" spacing="100.0">
			<children>
				<Button fx:id="cancelButton" layoutX="276.0" layoutY="475.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="取消" />
				<Button fx:id="confirmButton" layoutX="505.0" layoutY="487.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="确认" />
			</children>
		</HBox>
	</children>
</AnchorPane>
